{
  "name": "quick-validate",
  "version": "0.0.3",
  "description": "Express.js middleware for quick configurable API validations",
  "main": "lib/index.js",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "babel-jest": "^25.2.6",
    "coveralls": "^3.0.11",
    "express": "^4.17.1",
    "jest-cli": "^25.2.7"
  },
  "scripts": {
    "test": "jest --coverage",
    "prepare": "babel src --out-dir lib",
    "prepublishOnly": "babel src --out-dir lib"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/*.{js,jsx}",
      "!lib/*",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  },
  "keywords": [
    "express",
    "validation",
    "api",
    "node.js",
    "middleware"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/moyukhbera/quick-validate.git"
  },
  "author": "Moyukh Bera",
  "license": "MIT"
}